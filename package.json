{
  "name": "middlepapah",
  "version": "1.0.0",
  "description": "Middleware untuk express js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asruldev/middlepapah.git"
  },
  "keywords": [
    "Express",
    "Nodejs",
    "middleware"
  ],
  "author": "Asrul Harahap <talkasrul@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/asruldev/middlepapah/issues"
  },
  "homepage": "https://github.com/asruldev/middlepapah#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4"
  }
}
